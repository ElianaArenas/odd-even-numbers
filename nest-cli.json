{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/odd_even_numbers/src",
  "monorepo": true,
  "root": "apps/odd_even_numbers",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/odd_even_numbers/tsconfig.app.json"
  },
  "projects": {
    "odd_even_numbers": {
      "type": "application",
      "root": "apps/odd_even_numbers",
      "entryFile": "main",
      "sourceRoot": "apps/odd_even_numbers/src",
      "compilerOptions": {
        "tsConfigPath": "apps/odd_even_numbers/tsconfig.app.json"
      }
    },
    "validation-app": {
      "type": "application",
      "root": "apps/validation-app",
      "entryFile": "main",
      "sourceRoot": "apps/validation-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/validation-app/tsconfig.app.json"
      }
    }
  }
}